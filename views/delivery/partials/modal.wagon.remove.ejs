<div class="modal fade" id="ModalWagonRemove">
  <div class="modal-dialog">
    <div class="modal-content">
      <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
        <h4 class="modal-title">Wagon: <span id="removeRailcarNumber"></span></h4>
      </div>
      <form action="" method="" id="RailcarFormRemove">
	      <div class="modal-body">
	      	<input type="hidden" name="spot" />
	      	<input type="hidden" name="id"/>
					<table class="table table-hover table-striped">
						<!-- <caption>Wagon</caption> -->
						<tbody>
							<tr>
								<td><%= __('Modal.Wagon.Spot') %></td><td id="removeRailcarSpot"></td>
							</tr>
							<tr>
								<td><%= __('Modal.Wagon.Train') %></td><td id="removeRailcarTrain"></td>
							</tr>
							<tr>
								<td><%= __('Modal.Wagon.Delivery') %></td><td id="removeRailcarDelivery"></td>
							</tr>
							<tr>
								<td><%= __('Modal.Wagon.Product') %></td><td id="removeRailcarProduct"></td>
							</tr>
							<tr>
								<td><%= __('Modal.Wagon.BillOfLading') %></td><td id="removeRailcarBillOfLading"></td>
							</tr>
						</tbody>
					</table>
	      </div>
	      <div class="modal-footer">
	        <button type="button" class="btn btn-default" data-dismiss="modal"><%= __('Delivery.Modal.Close') %></button>
	        <button type="submit" id="removeCarBtn" class="btn btn-danger"><%= __('Delivery.Modal.RemoveCar') %></button>
	      </div>
	    </form>
    </div>
  </div>
</div>